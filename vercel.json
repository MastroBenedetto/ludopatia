{
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/superenalotto", "dest": "api/superenalotto.py" },
    { "src": "/api/lotto", "dest": "api/lotto.py" },
    { "src": "/(.*)", "dest": "dist/$1" }
  ]
}